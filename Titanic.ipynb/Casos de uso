            "text/html": [
              "\n",
              "  <div id=\"df-ddf848ef-85a2-4308-b7cf-3af46c087af2\" class=\"colab-df-container\">\n",
              "    <div>\n",
              "<style scoped>\n",
              "    .dataframe tbody tr th:only-of-type {\n",
