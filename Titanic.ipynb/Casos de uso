    {
      "cell_type": "code",
      "source": [
        "#Obtenemos el total de la familia\n",
        "data['Family_Size'] = data['SibSp'] + data['Parch']"
      ],
      "metadata": {
        "id": "g_GQ_4lEwPrA"
      },
